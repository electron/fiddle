{
  "name": "electron-fiddle",
  "private": true,
  "productName": "Electron Fiddle",
  "version": "0.37.1",
  "description": "The easiest way to get started with Electron",
  "repository": "https://github.com/electron/fiddle",
  "homepage": "https://electronjs.org/fiddle",
  "main": "./.webpack/main",
  "scripts": {
    "contributors": "tsx ./tools/contributors.ts",
    "lint:style": "stylelint \"./src/less/*.less\"",
    "lint:ts": "eslint \"./**/*.{ts,tsx}\"",
    "lint:js": "eslint \"./**/*.js\"",
    "lint:markdown": "markdownlint-cli2 \"*.md\"",
    "lint:templates": "standard \"static/**/*.js\"",
    "lint:links": "lint-roller-markdown-links --fetch-external-links \"*.md\"",
    "lint:fix": "npm-run-all \"lint:!(fix|links) -- --fix\"",
    "lint": "npm-run-all \"lint:!(fix)\"",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "tsx ./tools/clean-webpack.ts && electron-forge start --",
    "test": "vitest run",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit --outputFile=./reports/report.xml",
    "test:report": "vitest run --reporter=json --outputFile=report.json | true",
    "tsc": "tsc --noEmit -p .",
    "electron-releases": "tsx ./tools/fetch-releases.ts",
    "postinstall": "husky && npm run electron-releases"
  },
  "engines": {
    "node": ">= 22.12.0"
  },
  "keywords": [
    "Electron",
    "Fiddle",
    "Tutorial"
  ],
  "author": "Felix Rieseberg <felix@felixrieseberg.com>",
  "license": "MIT",
  "config": {
    "forge": "./forge.config.ts"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/popover2": "^0.12.2",
    "@blueprintjs/select": "^3.15.0",
    "@electron/fiddle-core": "^2.0.1",
    "@octokit/rest": "^17.0.0",
    "@sentry/electron": "^5.11.0",
    "algoliasearch": "^4.12.0",
    "classnames": "^2.2.6",
    "commander": "^7.1.0",
    "electron-default-menu": "^1.0.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^9.1.0",
    "getos": "^3.2.1",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "monaco-editor": "^0.21.3",
    "namor": "^2.0.2",
    "node-watch": "^0.7.3",
    "p-debounce": "^2.0.0",
    "package-json": "^7.0.0",
    "parse-env-string": "^1.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-mosaic-component": "^4.1.1",
    "react-window": "^1.8.10",
    "semver": "^7.3.4",
    "shell-env": "^3.0.1",
    "tmp": "0.2.1",
    "tslib": "^2.6.0",
    "tsx": "^4.20.3",
    "update-electron-app": "^3.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.8.1",
    "@electron-forge/maker-deb": "7.8.1",
    "@electron-forge/maker-rpm": "7.8.1",
    "@electron-forge/maker-squirrel": "7.8.1",
    "@electron-forge/maker-zip": "7.8.1",
    "@electron-forge/plugin-fuses": "7.8.1",
    "@electron-forge/plugin-webpack": "7.8.1",
    "@electron-forge/publisher-github": "7.8.1",
    "@electron/fuses": "^1.6.1",
    "@electron/lint-roller": "^3.1.2",
    "@hitarth-gg/devtron": "^0.0.0-development.8",
    "@octokit/core": "^3.5.1",
    "@reforged/maker-appimage": "^5.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/fs-extra": "^9.0.7",
    "@types/getos": "^3.0.1",
    "@types/node": "~22.10.7",
    "@types/parse-env-string": "^1.0.2",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.11",
    "@types/react-window": "^1.8.8",
    "@types/semver": "^7.3.4",
    "@types/tmp": "0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.2",
    "@vitest/coverage-v8": "3.2.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "36.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tsdoc": "^0.3.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "less": "^4.1.1",
    "less-loader": "^11.0.0",
    "lint-staged": "^15.2.7",
    "log-symbols": "^6.0.0",
    "markdownlint-cli2": "^0.18.0",
    "mini-css-extract-plugin": "^2.6.1",
    "monaco-editor-webpack-plugin": "2.1.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.25",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.0",
    "resolve-url-loader": "^5.0.0",
    "standard": "^17.1.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "webpack": "^5.94.0"
  },
  "lint-staged": {
    "./**/*.js": [
      "npm run lint:js -- --fix"
    ],
    "./**/*.{ts,tsx}": [
      "npm run lint:ts -- --fix"
    ],
    "./static/**/*.js": [
      "npm run lint:templates -- --fix"
    ],
    "./src/less/*.less": [
      "npm run lint:style -- --fix"
    ]
  },
  "resolutions": {
    "@electron-forge/maker-base": "7.8.1",
    "@electron-forge/shared-types": "7.8.1",
    "@electron/windows-sign": "github:electron/windows-sign#node-22"
  }
}
